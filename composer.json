{
    "name": "statamic/cli",
    "description": "Statamic CLI Tool",
    "keywords": ["statamic"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^6.5.5|^7.0.1",
        "laravel/prompts": "^0.1.3",
        "symfony/console": "^4.0|^5.0|^6.0",
        "symfony/process": "^4.2|^5.0|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "bin": [
        "bin/statamic"
    ],
    "autoload": {
        "psr-4": {
            "Statamic\\Cli\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Statamic\\Cli\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
